<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="wrap_content"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity"
    android:id="@+id/RelativeLayout1">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Text_Diaro"
        android:id="@+id/Diario"
        android:onClick="HacerDiario"
        android:nestedScrollingEnabled="false"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Button_Historico"
        android:id="@+id/Historico"
        android:layout_below="@+id/Diario"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/Diario"
        android:layout_alignEnd="@+id/Diario" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/TextCAU"
        android:id="@+id/CAU"
        android:layout_below="@+id/Historico"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/Historico"
        android:layout_alignEnd="@+id/Historico" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/TextCliente"
        android:id="@+id/Cliente"
        android:layout_below="@+id/CAU"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/CAU"
        android:layout_alignEnd="@+id/CAU" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/TextSolucion"
        android:id="@+id/Solucion"
        android:layout_below="@+id/Cliente"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/Cliente"
        android:layout_alignEnd="@+id/Cliente" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Button_Gasolinera"
        android:id="@+id/Gasolinera"
        android:layout_below="@+id/Solucion"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/Solucion"
        android:layout_alignEnd="@+id/Solucion"
        android:onClick="HecharGasoil" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="VerCoches"
        android:text="@string/btnCoche"
        android:id="@+id/Coche"
        android:layout_below="@+id/Gasolinera"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/Gasolinera"
        android:layout_alignEnd="@+id/Gasolinera"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="false"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Button_Salir"
        android:id="@+id/Salir"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="Salir" />
</RelativeLayout>
